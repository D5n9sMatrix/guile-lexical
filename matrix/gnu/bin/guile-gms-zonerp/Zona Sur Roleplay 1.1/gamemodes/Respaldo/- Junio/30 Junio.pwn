			format(string, sizeof(string), "Felicidades has comprado éste negocio por $%d.", DealershipStatistics[dealershipid][dealership_name], DealershipStatistics[dealershipid][dealership_price]);
			SendClientMessage(playerid, color_yellow, string);
			SetPVarInt(playerid, "HasDealership", dealershipid);
			SaveDealership(dealershipid);
			return 1;
		}
		else return SendClientMessage(playerid, color_grey, "Usted no tiene dinero.");
	}
	else { SendClientMessage(playerid, color_grey, "Éste concesionario ya tiene propietario. !"); }
	return 1;
}

