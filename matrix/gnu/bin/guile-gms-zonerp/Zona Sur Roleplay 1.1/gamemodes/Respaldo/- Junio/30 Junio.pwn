}
}
else
{
SendClientMessage(playerid, COLOR_BLANCO, "Nesesitas estar en una Grua para Remolcar!");
return 1;
}
}
else
{
SendClientMessage(playerid, COLOR_BLANCO, "No puedes remolcar a pie!");
return 1;
}
}
return 1;
}
