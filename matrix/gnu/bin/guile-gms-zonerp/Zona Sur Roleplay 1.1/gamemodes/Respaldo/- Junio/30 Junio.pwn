new String2[200];
format(String2,sizeof(String2),"El banco te descontó {34CE00}2000${FFFFFF} por el prestamo , todavía resta:{34CE00}%d",PI[i][PrestamoP]-20000);
Mensaje(i,COLOR_BLANCO,String2);
if(PI[i][PrestamoP] == 20000) {
PI[i][PrestamoP] = 0;
PI[i][Prestamo] = 0;
Mensaje(i,COLOR_BLANCO,"{CE0000}-{FFFFFF} Terminaste de pagar el {CE0000}prestamo.");
}
}
if(PI[i][SubNivel] == 12) {
PI[i][Nivel]++;
SetPlayerScore(i,GetPlayerScore(i)+1);
Mensaje(i,COLOR_BLANCO, "{FFFFFF}Subiste un {00B7FF}nivel.");
PI[i][SubNivel] = 0; }
}
}
return 1;
}
