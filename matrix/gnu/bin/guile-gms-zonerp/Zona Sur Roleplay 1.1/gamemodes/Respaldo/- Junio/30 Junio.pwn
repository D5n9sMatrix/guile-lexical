	    return SendClientMessage(playerid, color_white, "/borrarconcesionario [ID]");

	if(id < 1 || id > MAX_DEALERSHIPS) return SendClientMessage(playerid, color_grey, "ID INVALIDA.");
	if(dealership_taken[id] == true)
	{
